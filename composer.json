{
    "name": "jdarkins/pulse-throttled",
    "description": "A Laravel Pulse card to track users who are being throttled by your Rate Limiting ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Darkins",
            "email": "jdarkins95@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0|^12.0",
        "laravel/pulse": "^1.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jdarkins\\PulseThrottled\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jdarkins\\PulseThrottled\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jdarkins\\PulseThrottled\\PulseThrottledServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}