{
    "name": "jdarkins/pulse-throttled",
    "description": "A Laravel Pulse card to track users & URL's that are being throttled by your Rate Limiters",
    "keywords": [
        "laravel",
        "pulse",
        "laravel-pulse",
        "throttle",
        "rate limit"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Darkins",
            "email": "jdarkins95@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0|^12.0",
        "laravel/pulse": "^1.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jdarkins\\PulseThrottled\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jdarkins\\PulseThrottled\\PulseThrottledServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}